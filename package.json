  "dependencies": {
    "backbone": "^1.2.3",
    "express": "^4.13.4",
    "jquery": "^2.2.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "handlebars": "^4.0.5",
    "hbsfy": "^2.5.0",
    "watchify": "^3.7.0"
  }
}
